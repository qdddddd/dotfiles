{
    "suggest.minTriggerInputLength": 1,
    "suggest.enablePreview": false,
    "suggest.echodocSupport": true,
    "suggest.languageSourcePriority": 1,
    "suggest.numberSelect": false,
    "suggest.snippetIndicator": "▸",
    "suggest.noselect": false,
    "suggest.detailField": "menu",
    "suggest.labelMaxLength": 60,
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "signature.triggerSignatureWait": 50,
    "signature.target": "float",
    "diagnostic.errorSign": "✗",
    "diagnostic.warningSign": "▲",
    "diagnostic.infoSign": "●",
    "diagnostic.hintSign": "◇",
    "diagnostic.checkCurrentLine": true,
    "diagnostic.signOffset": 1,
    "list.nextKeymap": "<C-n>",
    "list.previousKeymap": "<C-p>",
    "languageserver": {
        "clangd": {
            "command": "clangd",
            "args": [
                "-log=error",
                "-pretty",
                "-pch-storage=memory",
                "-function-arg-placeholders=1",
                "-header-insertion=never",
                "-clang-tidy",
                "-completion-style=detailed",
                "-suggest-missing-includes",
                "-j=4"
            ],
            "rootPatterns": [
                "compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "filetypes": [
                "c",
                "cpp",
                "objc",
                "objcpp",
                "cuda"
            ]
        }
    },
    "coc.preferences.formatOnSaveFiletypes": [
        "c",
        "cpp",
        "java",
        "vim",
        "yaml",
        "python",
        "json"
    ],
    "coc.preferences.formatOnType": false,
    "pairs.enableCharacters": [
        "(",
        "[",
        "{",
        "'",
        "\"",
        "`"
    ],
    "python.formatting.provider": "yapf",
    "python.jediEnabled": false,
    "python.analysis.diagnosticEnabled": true,
    "python.linting.lintOnSave": true
}
