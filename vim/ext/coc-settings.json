{
    "suggest.minTriggerInputLength": 1,
    "suggest.languageSourcePriority": 1,
    "suggest.snippetIndicator": "",
    "suggest.noselect": false,
    "suggest.detailField": "preview",
    "suggest.labelMaxLength": 60,
    "suggest.removeDuplicateItems": true,
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "signature.triggerSignatureWait": 200,
    "signature.target": "float",
    "diagnostic.errorSign": "✗",
    "diagnostic.warningSign": "▲",
    "diagnostic.infoSign": "●",
    "diagnostic.hintSign": "◇",
    "diagnostic.checkCurrentLine": true,
    "diagnostic.showDeprecated": true,
    "diagnostic.level": "hint",
    "list.nextKeymap": "<C-n>",
    "list.previousKeymap": "<C-p>",
    "languageserver": {
        "csharp-ls": {
            "command": "csharp-ls",
            "args": ["--loglevel", "log"],
            "filetypes": [
                "cs"
            ],
            "rootPatterns": [
                ".git/",
                "*.sln",
                "*.csproj"
            ],
            "trace.server": "verbose"
        }
    },
    "clangd.arguments": [
        "-log=error",
        "-pretty",
        "-pch-storage=memory",
        "-function-arg-placeholders=1",
        "-header-insertion=never",
        "-clang-tidy",
        "-completion-style=detailed",
        "-suggest-missing-includes",
        "-j=6"
    ],
    "coc.source.buffer.ignoreGitignore": true,
    "coc.preferences.bracketEnterImprove": true,
    "semanticTokens.enable": true,
    "pairs.enableCharacters": [
        "(",
        "[",
        "{",
        "'",
        "\"",
        "`"
    ],
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "~/.local/bin/black",
    "python.formatting.blackArgs": [
        "--line-length", "120"
    ],
    "python.linting.lintOnSave": true,
    "Lua.diagnostics.globals": [
        "vim"
    ],
    "coc.preferences.formatOnSave": false,
    "[c][cs][cpp][java][vim][yaml][python]": {
        "coc.preferences.formatOnSave": true,
    },
    "inlayHint.enable": false,
    "coc.preferences.formatOnType": false,
    "coc.preferences.extensionUpdateCheck": "daily"
}
